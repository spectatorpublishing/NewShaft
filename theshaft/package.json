{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@storybook/addon-info": "^4.1.12",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.2.1",
    "carousel": "0.0.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "dotenv-webpack": "^1.6.0",
    "express": "^4.16.3",
    "fs": "^0.0.1-security",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.52.0",
    "mysql": "^2.16.0",
    "npm": "^6.4.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-image-lightbox": "^5.1.0",
    "react-map-gl": "^4.0.7",
    "react-responsive-carousel": "^3.1.46",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "request-promise": "^4.2.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.1.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.0.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.17.3",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.6.3",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "superagent": "^4.0.0-beta.5",
